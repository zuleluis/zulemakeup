  <servlet-mapping>
    <servlet-name>RegistrarCita</servlet-name>
    <url-pattern>/RegistrarCita</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>MapeoListas</servlet-name>
    <url-pattern>/MapeoListas</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>BuscarCita</servlet-name>
    <url-pattern>/BuscarCita</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServiciosCliente</servlet-name>
    <url-pattern>/ServiciosCliente</url-pattern>
  </servlet-mapping>

</web-app>
