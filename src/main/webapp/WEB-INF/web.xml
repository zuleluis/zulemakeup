<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>ListaClientes</servlet-name>
    <servlet-class>controlador.clientes.ListaClientes</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>RegistrarCliente</servlet-name>
    <servlet-class>controlador.clientes.RegistrarCliente</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>BuscarCliente</servlet-name>
