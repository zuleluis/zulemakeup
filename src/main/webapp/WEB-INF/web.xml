    <servlet-class>controlador.citas.MapeoListas</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>BuscarCita</servlet-name>
    <servlet-class>controlador.citas.BuscarCita</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ServiciosCliente</servlet-name>
    <servlet-class>controlador.citas.ServiciosCliente</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ListaClientes</servlet-name>
    <url-pattern>/ListaClientes</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>RegistrarCliente</servlet-name>
    <url-pattern>/RegistrarCliente</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>BuscarCliente</servlet-name>
    <url-pattern>/BuscarCliente</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>EliminarCliente</servlet-name>
